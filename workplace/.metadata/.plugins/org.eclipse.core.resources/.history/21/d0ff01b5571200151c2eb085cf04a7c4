package learnJAVA;

public class javaString {

	private static String con;

	public static void main(String[] args) {
		String first = "Hello";
		String last = "hi";
		int length = first.length();
		con = first.concat(last);
		String name = "Hello";
		System.out.println(first==name);
		System.out.println(first.equals(name));
	}

}
