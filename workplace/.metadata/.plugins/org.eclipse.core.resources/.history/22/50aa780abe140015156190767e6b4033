package learnJAVA;

class gen<T> {
	T ob;
	
	gen(T o){ob = o;}
	
	T getob(){return ob;}
	
	void showType(){
		System.out.println("Type of T is :" + ob.getClass().getName());
	}
}

public class gendemo {

	public static void main(String[] args) {
		// TODO using class template
		gen<String> str = new gen<String>("Anurag");
		str.getClass();
	}

}
