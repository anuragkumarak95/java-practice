package learnJAVA;

class math{
	double a,b;
	math(){a=0;
	b=0;}
	
	math(double a,double b){
		this.a = a;
		this.b = b;
	}
	
	double div(){
		return a / b;
	}
}

public class trycatchexp {

	public static void main(String[] args) {
		// Try and Catch example
		math m1 = new math(1,0);
		try{
		double a=1,b=0,c=a/b;
		System.out.print(c);

		}
		catch(ArithmeticException e){
			System.out.println(e);
		}
	}

}
