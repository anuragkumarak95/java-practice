package learnJAVA;


class box2{
	double wd;
	double ht;
	
	double vol(){
		return wd*ht;
	}
}
public class classdemo3 {

	public static void main(String[] args) {
		box2 mybox1,mybox2;
		mybox2 = new box2();
		mybox2.wd = 25;
		mybox2.ht = 30;
		
		mybox1 = new box2();
		mybox1.wd = 10;
		mybox1.ht = 20;
		double res = mybox1.vol();
		System.out.println("Result: " + res);
	}

}
