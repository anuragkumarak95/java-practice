package learnJAVA;

import Experimentals.cons;

class box{
	double width;
	String name;
}
public class Demo {

	public static void main(String[] args) {
		box b1;
		b1 = new box();//malloc keywword is used in c rather than new for same purpose.
		cons.give("Value of width is: " + b1.width);
		cons.give("Value of name is: " + b1.name);
	}

}
