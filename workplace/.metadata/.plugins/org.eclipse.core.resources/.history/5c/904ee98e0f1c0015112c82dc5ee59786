<%@ page import="java.util.ArrayList" %>
<%@ page import="java.sql.*" %>
<%
String txta = request.getParameter("texta");
int len = txta.length();
ArrayList<String> querylst = new ArrayList<>();
String str = "";
char chr = ' ';
for(int i=0;i<len;i++){
	chr = txta.charAt(i);
	if(chr==';'){
		querylst.add(str);
		str="";
	}
	else{str=str+chr;}
	
}
if(str.equals("")){}else{querylst.add(str);}

out.println(querylst);

%>


