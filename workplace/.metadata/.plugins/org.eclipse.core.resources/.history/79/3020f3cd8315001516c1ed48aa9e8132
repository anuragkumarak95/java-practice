package learnJAVA;

class multibox{
	private double ln,wd,ht;
	
	multibox(double ln,double wd,double ht){
		this.ln = ln;
		this.wd = wd;
		this.ht = ht;
	}
	
	multibox(double all){
		ln = wd = ht = all;
	}
	
	double volume(){
		return ln*wd*ht;}
	
}

public class demomultiinheritance {

	public static void main(String[] args) {
		// TODO using multi inheritance and private instances.

	}

}
