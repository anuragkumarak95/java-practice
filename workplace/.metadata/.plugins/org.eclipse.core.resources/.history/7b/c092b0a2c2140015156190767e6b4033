package learnJAVA;

class boxer{
	double ln,wd,ht;
	
	boxer(){
		ln = -1;
		wd = -1;
		ht = -1;
	}
	
	boxer(double ln,double wd,double ht){
		this.ln = ln;
		this.wd = wd;
		this.ht = ht;
	}
	
	boxer(double ln){
		this.ln = ln;
	}
	
	boxer(boxer b){
		ln = b.ln;
		ht = b.ht;
		wd = b.wd;
	}
	void show(){System.out.println(ln+" "+wd+" "+ht);}
	
}

public class overloadedconstructor {

	public static void main(String[] args) {
		// TODO Constructor overloading
		boxer bx1 = new boxer(),bx2 = new boxer(1,2,3),bx3 = new boxer(1),bx4 = new boxer(bx2);
		bx1.show();
		bx2.show();
		bx3.show();
		bx4.show();
		

	}

}
