package learnJAVA;

abstract class bank{
	abstract double ROI();
	void servive(){
		System.out.println("1. Credit Card");
		System.out.println("2. Savings Account");
	}
}

class hdfc extends bank{
	double ROI(){
		return 10;
	}
}

public class abstract_bank {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
