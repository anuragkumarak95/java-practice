package learnJAVA;

import Experimentals.cons;

class box2{
	double wd;
	double ht;
	
	double vol(){
		return wd*ht;
	}
}
public class boxdemo3 {

	public static void main(String[] args) {
		box2 mybox1,mybox2;
		mybox1 = new box2();
		mybox1.wd = 20;
		mybox1.ht = 20;
		double res = mybox1.vol();
		cons.give("Result: " + res);
	}

}
