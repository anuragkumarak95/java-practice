package Experimentals;

import java.util.ArrayList;

public class using_cons_tokenize {

	public static void main(String[] args) {
		// TODO Using the cons.tokenixe funtion that i created which converts a String into tokens and add in order in the arraylist.
		cons.give("Enter the String below: ");
		String str = cons.take();
		ArrayList<String> arrlist = new ArrayList<String>();
		cons.tokenize(str,arrlist);
		cons.give(arrlist);
	}

}
